<div class="wmail-mailbox-status">
  <%= render 'mailbox_status' %>
</div>

<div class="left wmail-mailbox-list">
  <%= render 'folder_list', :folder_lists => @folders_count_hash%>
</div>

<div class="wmail-mailbox wmail-inbox">
  <table width="100%">
    <thead>
      <tr>
        <th class="wmail-sender">From</th>
        <th class="wmail-subject">Subject</th>
        <th class="wmail-date">Date</th>
      </tr>
    </thead>
    <tbody id="wmail-mail-container">
      <%= render 'mail_list', :mails => @inbox.reverse %>
    </tbody>
  </table>
</div>